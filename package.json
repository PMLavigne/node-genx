{
  "name": "genx",
  "version": "1.2.1",
  "description": "Evented XML generation using the Genx C library",
  "keywords": [
    "xml",
    "generation",
    "generator",
    "builder"
  ],
  "homepage": "http://node-genx.wezm.net/",
  "author": "Wesley Moore <wes@wezm.net> (http://www.wezm.net/)",
  "repository": "git://github.com/wezm/node-genx.git",
  "main": "./lib/genx",
  "engines": {
    "node": ">=0.8.0"
  },
  "license": "BSD-3-Clause",
  "licenses": [
    {
      "type": "BSD",
      "url": "https://github.com/wezm/node-genx/blob/master/LICENSE"
    }
  ],
  "scripts": {
    "clean": "(mkdir build || rm -rf build) && mkdir -p build",
    "cmake": "cd build && cmake .. && make -j6",
    "cmake-debug": "source ~/.nvm/nvm.sh && nvm use 4.2.3 && cd build && cmake -DCMAKE_BUILD_TYPE=Debug .. && make -j6",
    "test": "mocha"
  },
  "optionalDependencies": {
    "node-cmake": ">=1.1.0"
  },
  "devDependencies": {
    "mocha": ">=1.8.1",
    "should": ">=1.2.1"
  },
  "dependencies": {
    "nan": ">=2.1.0"
  }
}
